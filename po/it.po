#: ../usr/lib/sambashare/sambashare.py:58
#: ../usr/share/kde4/services/kcm_sambashare.desktop.in.h:1
#: ../usr/share/applications/sambashare.desktop.in.h:1
msgid "Samba share"
msgstr "Condivisioni Samba"

#: ../usr/lib/sambashare/sambashare.py:59
msgid "Create samba share"
msgstr "Crea una condivisione Samba"

#: ../usr/lib/sambashare/sambashare.py:61
msgid "Name"
msgstr "Nome"

#: ../usr/lib/sambashare/sambashare.py:62
msgid "Path"
msgstr "Percorso"

#: ../usr/lib/sambashare/sambashare.py:63
msgid "Comment"
msgstr "Commento"

#: ../usr/lib/sambashare/sambashare.py:64
msgid "Read only"
msgstr "Sola lettura"

#: ../usr/lib/sambashare/sambashare.py:65
msgid "Public"
msgstr "Pubblico"

#: ../usr/lib/sambashare/sambashare.py:117
msgid "Select directory to share"
msgstr "Seleziona una cartella da condividere"

#: ../usr/lib/sambashare/sambashare.py:126
msgid "Remove share"
msgstr "Rimuovi condivisione"

#: ../usr/lib/sambashare/sambashare.py:127
#, python-format
msgid ""
"Are you sure you want to remove the following share:\n"
"\n"
"'%(share)s'"
msgstr ""
"Sei sicuro di voler rimuovere la seguente condivisione:\n"
"\n"
"'%(share)s'"

#: ../usr/lib/sambashare/sambashare.py:136
msgid "Create share"
msgstr "Crea condivisione"

#: ../usr/lib/sambashare/sambashare.py:157
#, python-format
msgid "Could not remove share: '%(share)s'"
msgstr "Impossibile rimuovere: '%(share)s'"

#: ../usr/lib/sambashare/sambashare.py:159
#, python-format
msgid "Could not create share: '%(share)s'"
msgstr "Impossibile creare: '%(share)s'"

#: ../usr/lib/sambashare/sambashare.py:161
#, python-format
msgid ""
"Share successfully %(action)s:\n"
"\n"
"'%(share)s' on %(path)s"
msgstr ""
"Condivisione creata con successo: %(action)s:\n"
"\n"
"'%(share)s' su %(path)s"

#: ../usr/lib/sambashare/usershare.py:84
#, python-format
msgid ""
"Cannot create share.\n"
"Share already exists: %(share)s"
msgstr ""
"Impossibile creare la condivisione.\n"
"La condivisione risulta gi√† esistente: '%(share)s'"

#: ../usr/lib/sambashare/usershare.py:86
#, python-format
msgid ""
"Cannot create share.\n"
"Path does not exist: %(path)s"
msgstr ""
"Impossibile creare la condivisione.\n"
"Il percorso non esiste: %(path)s"

#: ../usr/lib/sambashare/usershare.py:98
#, python-format
msgid ""
"You do not have sufficient permission to create a share on:\n"
"%(path)s"
msgstr ""
"Non si possiedono diritti sufficienti per creare una condivisione di: "
"%(path)s"

#: ../usr/lib/sambashare/usershare.py:116
#, python-format
msgid "You do not have sufficient permission on path: %(path)s"
msgstr "Non si possiedono sufficienti diritti sul percorso: %(path)s"

#: ../usr/share/kde4/services/kcm_sambashare.desktop.in.h:2
#: ../usr/share/applications/sambashare.desktop.in.h:2
msgid "Create samba shares"
msgstr "Crea una condivisione Samba"
