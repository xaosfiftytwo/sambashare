#: ../usr/lib/sambashare/sambashare.py:62
#: ../usr/share/kde4/services/kcm_sambashare.desktop.in.h:1
#: ../usr/share/applications/sambashare.desktop.in.h:1
msgid "Samba share"
msgstr "Samba"

#: ../usr/lib/sambashare/sambashare.py:63
msgid "Create samba share"
msgstr "Создать общий ресурс Samba"

#: ../usr/lib/sambashare/sambashare.py:65
msgid "Name"
msgstr "Имя"

#: ../usr/lib/sambashare/sambashare.py:66
msgid "Path"
msgstr "Путь"

#: ../usr/lib/sambashare/sambashare.py:67
msgid "Comment"
msgstr "Комментарий"

#: ../usr/lib/sambashare/sambashare.py:68
msgid "Read only"
msgstr "Только чтение"

#: ../usr/lib/sambashare/sambashare.py:69
msgid "Public"
msgstr "Публичная"

#: ../usr/lib/sambashare/sambashare.py:121
msgid "Select directory to share"
msgstr "Выберите каталог для общего доступа"

#: ../usr/lib/sambashare/sambashare.py:128
msgid "Remove share"
msgstr "Удалить общий ресурс"

#: ../usr/lib/sambashare/sambashare.py:129
#, python-format
msgid ""
"Are you sure you want to remove the following share:\n"
"\n"
"'%(share)s'"
msgstr ""
"Вы действительно:\n"
"\n"
"'%(share)s'"

#: ../usr/lib/sambashare/sambashare.py:138
msgid "Create share"
msgstr "Создать общий ресурс"

#: ../usr/lib/sambashare/sambashare.py:173
#, python-format
msgid "Could not remove share: '%(share)s'"
msgstr "Не удалось удалить общий ресурс: '%(share)s'"

#: ../usr/lib/sambashare/sambashare.py:175
#, python-format
msgid "Could not create share: '%(share)s'"
msgstr "Не удается создать общий ресурс: '%(share)s'"

#: ../usr/lib/sambashare/sambashare.py:177
#, python-format
msgid ""
"Share successfully %(action)s:\n"
"\n"
"'%(share)s' on %(path)s"
msgstr ""
"Общий ресурс успешно %(action)s:\n"
"\n"
"'%(share)s' в %(path)s"

#: ../usr/lib/sambashare/usershare.py:85
#, python-format
msgid ""
"Cannot create share.\n"
"Share name %(share)s is already a valid system user name."
msgstr ""
"Нельзя создать раздачу.\n"
"Имя раздачи %(share)s уже действующее имя пользователя системы."

#: ../usr/lib/sambashare/usershare.py:87
#, python-format
msgid ""
"Cannot create share.\n"
"Share already exists: %(share)s"
msgstr ""
"Не удалось создать общий ресурс.\n"
"Общий ресурс уже существует: %(share)s"

#: ../usr/lib/sambashare/usershare.py:89
#, python-format
msgid ""
"Cannot create share.\n"
"Path does not exist: %(path)s"
msgstr ""
"Не удалось создать общий ресурс.\n"
"Неверный путь: %(path)s"

#: ../usr/lib/sambashare/usershare.py:101
#, python-format
msgid ""
"You do not have sufficient permission to create a share on:\n"
"%(path)s"
msgstr ""
"У Вас нет соответствующих прав для создания общего ресурса в каталоге:\n"
"%(path)s"

#: ../usr/lib/sambashare/usershare.py:113
msgid "Failed to create new share: {} on {}."
msgstr "Не удалось создать новую раздачу: {} на {}."

#: ../usr/lib/sambashare/usershare.py:122
#, python-format
msgid "You do not have sufficient permission on path: %(path)s"
msgstr "У Вас нет соответствующих прав в каталоге: %(path)s"

#: ../usr/lib/sambashare/usershare.py:129
msgid "Failed to remove share: {} from {}."
msgstr "Не удалось удаление раздачи {} из {}."

#: ../usr/share/kde4/services/kcm_sambashare.desktop.in.h:2
#: ../usr/share/applications/sambashare.desktop.in.h:2
msgid "Create samba shares"
msgstr "Создать общие папки Samba"
